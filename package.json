{
  "name": "lumosity_leaderboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:ci": "cypress run --browser chrome",
    "test:report:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "test:report:generate": "marge cypress/reports/merged-report.json -f report -o cypress/reports/html",
    "test:report": "npm run test:report:merge && npm run test:report:generate",
    "test:clean": "rm -rf cypress/reports cypress/videos cypress/screenshots",
    "clean": "rm -rf .next",
    "clean:next": "rm -rf .next",
    "clean:prisma": "rm -rf node_modules/.prisma node_modules/@prisma/client",
    "clean:ts": "find . -name '*.tsbuildinfo' -delete",
    "clean:all": "rm -rf .next node_modules package-lock.json *.tsbuildinfo && npm install",
    "clean:install": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "next": "^15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "pg": "^8.17.2",
    "postcss": "^8.4.49",
    "prisma": "^7.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "cypress": "^15.9.0",
    "eslint": "^9.39.3",
    "eslint-config-next": "^15.5.12",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "tsx": "^4.21.0"
  }
}
